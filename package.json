{
  "name": "ez-order",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "npm run catalog:validate && npm run catalog:sync && npm run -w @ez-order/web build",
    "catalog:sync": "node scripts/sync-catalog-fallback.mjs",
    "catalog:validate": "node scripts/validate-catalog.mjs",
    "dev": "npm run -w @ez-order/web dev",
    "dev:web": "npm run -w @ez-order/web dev",
    "lint": "npm run -w @ez-order/web lint",
    "preview": "npm run -w @ez-order/web preview"
  }
}
